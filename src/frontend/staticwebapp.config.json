{
  "navigationFallback": {
    "rewrite": "/index.html",
    "exclude": ["/_next/*", "/api/*", "*.{css,scss,js,png,gif,ico,jpg,svg,json,woff,woff2,ttf,eot}"]
  },
  "routes": [
    {
      "route": "/api/*",
      "allowedRoles": ["anonymous"]
    }
  ],
  "globalHeaders": {
    "X-Frame-Options": "DENY",
    "X-Content-Type-Options": "nosniff",
    "Referrer-Policy": "strict-origin-when-cross-origin"
  },
  "platform": {
    "apiRuntime": "node:18"
  },
  "trailingSlash": "auto"
}
